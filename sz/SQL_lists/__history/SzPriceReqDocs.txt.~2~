/*Список деталей/материалов/комплектующих ОАСУПиИТ*/
---------------------
/*COLUMNS_START
<0>DEP_CODE</0><1>SZ_NUMBER</1><2>SZ_DATE</2><3>EMP_FIO</3><end>
COLUMNS_END*/

/*TITLES_START
<0>Отдел</0><1>№ док.</1><2>Дата</2><3>Исполнитель</3><end>
TITLES_END*/
--------------------

Select /*BeginFields*/ %s /*EndFields*/
From   Src.v_Sz_Price_Request t
Left  Join Stp.v_Users u
On    u.Id_User=t.i_User
      And u.Id_User_Add=t.I_User_Add
Left  Join Stp.V_Deps dep
On    Dep.Id_Dep=t.Sz_Dep_Id
      And Nvl(Dep.Date_Expitation, Sysdate)>=Sysdate
/*BeginFilter*/ %s /*EndFilter*/