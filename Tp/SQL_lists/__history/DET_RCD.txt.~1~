/*Список состава сборки*/
---------------------
/*COLUMNS_START
<0>Det_Num</0><1>Det_Name</1><end>
COLUMNS_END*/

/*TITLES_START
<0>Чертежный №</0><1>Наименование</1><end>
TITLES_END*/
--------------------
Select Distinct a.Det Det_Id, 
       a.Det_Txt Det_Num, 
       a.Nd Det_Name, 
       a.Kol 
From   Ogt.V$Spr_Rcd a
Where  /*FILTER_START*/ (1=1) /*FILTER_END*/
	