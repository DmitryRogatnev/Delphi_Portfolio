/*Список названий параметров для расчета*/
---------------------
/*COLUMNS_START
<0>Depends_Flag_Name</0><end>
COLUMNS_END*/

/*TITLES_START
<0>Зависит от</0><end>
TITLES_END*/
--------------------
Select ( Case Rownum 
           When 1 Then 'param'
           When 2 Then 'value'
         End ) Depends_Flag, 
       ( Case Rownum 
           When 1 then 'Значиния др. параметра'
           When 2 then 'Значeния параметра'
         End ) Depends_Flag_Name
From   Dual
Connect By Level < 3
Where  /*FILTER_START*/ (1=1) /*FILTER_END*/
	