/*Список видов работ*/
---------------------
/*COLUMNS_START
<0>Work_Type_Name</0><end>
COLUMNS_END*/

/*TITLES_START
<0>Вид работ</0><end>
TITLES_END*/
--------------------
Select t.Num Work_Type_Id, 
       t.Num_Gr_Vid_Rab, 
       t.Name_Gr_Vid_Rab,
       '('||t.Num_Gr_Vid_Rab||') '||Decode( t.Num_Gr_Vid_Rab, '00', 'БЕЗ УКАЗАНИЯ', t.Name_Gr_Vid_Rab ) Work_Type_Name
From   Ogt.SPR_GR_VID_RAB t
Where  /*FILTER_START*/ (1=1) /*FILTER_END*/
	